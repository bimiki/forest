

<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Dancing+Script:wght@700&display=swap" rel="stylesheet">

<h1 class="header">Like a forest</h1>

<h2 class="bluehand">listen to chilledcow</h2>

<div class="content">
<iframe height="200px" width="100%" src="https://www.youtube.com/embed/DWcJFNfaw9c"  allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"> </iframe>
</div>

      <p class="greyhand">YOUTUBE LIKE A FOREST</p>
      <th colspan="3"></th>

    <% @notes.each do |note| %>

        <!--<td><%= note.name %></td> -->
    <div class="bluehand">
      <p style="text-align: center;"><iframe  height="" width="" src="<%= note.enbedable_url %>" frameborder="1" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"> </iframe></p>

     <tr class="bluehand">
      <td><button><%= render "notes/upvote_link", note: note %></button></td>
      <td><button><%= render "notes/downvote_link", note: note %></button></td>
      <td><%= render "notes/votes_score", note: note %></td>
      <td><button><%= link_to 'Show', note %></button></td>
      <td><button><%= link_to 'Edit', edit_note_path(note) %></button></td>
      <td><button><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
      <td><button><%= link_to 'Add', new_note_path %></button></td>
      </tr>
    </div>
    <% end %>

